import java.util.Scanner;

class Main {

  public static void main(String[] args) {

    Scanner ler = new Scanner(System.in);

    int num,i;

    System.out.println("Digite a tabuada que deseja obter a multiplicação: ");

    num=ler.nextInt();

    System.out.println("A tabuada do numero "+num+" é:");

   for(i=1; i <=10; i++){

     System.out.println("            ****  "+num+" X "+i+" = "+num*i+"  ****");

   }

  }

}
